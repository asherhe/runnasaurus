<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <StartupObject>runnasaurus.Program</StartupObject>
    <IsPublishable>False</IsPublishable>
    <Authors>Asher He</Authors>
    <Title>Runnasaurus</Title>
    <Description>A clone of the Chrome dino game in the terminal</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Pastel" Version="4.0.2" />
  </ItemGroup>

</Project>
